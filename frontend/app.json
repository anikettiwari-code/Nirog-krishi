{
  "expo": {
    "name": "निरोग कृषि",
    "slug": "nirog-krishi",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.aniket.nirogkrishi",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "server"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      "react-native-fast-tflite"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}